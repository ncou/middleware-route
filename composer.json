{
  "name": "phapi/middleware-route",
  "description": "Middleware handling routing and dispatching to endpoints.",
  "keywords": [
    "middleware", "route", "router", "routing", "dispatch", "dispatcher", "phapi"
  ],
  "version": "1.0.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "http://phapi.github.io",
  "authors": [
    {
      "name": "Peter Ahinko",
      "email": "peter@ahinko.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "phapi/contract": "1.*",
    "phapi/exception": "1.*",
    "psr/http-message": "1.*"
  },
  "require-dev": {
    "phapi/http": "1.*",
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Phapi\\Middleware\\Route\\": "src/Phapi/Middleware/Route/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phapi\\Tests\\Middleware\\Route\\": "tests/Phapi/Middleware/Route/"
    }
  }
}